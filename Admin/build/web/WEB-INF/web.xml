<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.help.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>block</servlet-name>
        <servlet-class>com.help.block</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deActive</servlet-name>
        <servlet-class>com.help.deActive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>active</servlet-name>
        <servlet-class>com.help.active</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostActive</servlet-name>
        <servlet-class>com.help.PostActive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlacerDeactive</servlet-name>
        <servlet-class>com.help.PlacerDeactive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlacerActive</servlet-name>
        <servlet-class>com.help.PlacerActive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update</servlet-name>
        <servlet-class>com.help.Update</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>block</servlet-name>
        <url-pattern>/block</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deActive</servlet-name>
        <url-pattern>/deActive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>active</servlet-name>
        <url-pattern>/active</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostActive</servlet-name>
        <url-pattern>/PostActive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlacerDeactive</servlet-name>
        <url-pattern>/PlacerDeactive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlacerActive</servlet-name>
        <url-pattern>/PlacerActive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update</servlet-name>
        <url-pattern>/Update</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
